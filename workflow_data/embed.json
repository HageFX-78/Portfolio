{
    "content": "Test with embed",
    "embeds": [
        {
        "title": "[{repository_owner}/{repository}]",
        "description": ">>> [Repo link]({repository_url})\n [Commit link]({event_compare})",
        "color": 8518399,
        "fields": [
            {
            "name": "{commit_title}",
            "value": "{commit_description}"
            }
        ],
        "author": {
            "name": "{actor}",
            "url": "{actor_url}",
            "icon_url": "${actor_avatar}"
        },
        "footer": {
            "text": "Commit Time"
        },
        "timestamp": "{timestamp}",
        "image": {
            "url": "{actor_avatar}"
        },
        "thumbnail": {
            "url": "{actor_avatar}"
        }
        }
    ],
    "username": "GitHub Action",
    "attachments": []
}
