{
    "content": "",
    "embeds": [
        {
            "title": "[ {repository_owner}/{repository} ]",
            "description": "Event: {event_type}",
            "url": "{repository_url}",
            "color": 16740396,
            "fields": [
                {
                "name": "{commit_title}",
                "value": "{commit_description}"
                },
                {
                  "name": "Links",
                  "value": ">>> Commit: [{commit_id}]({event_compare})}) - "
                }
            ],
            "author": {
                "name": "{actor}",
                "url": "{actor_url}",
                "icon_url": "{actor_avatar}"
            },
            "footer": {
                "text": "Timestamp: "
            },
            "timestamp": "{timestamp}",
            "image": {
                "url": "{actor_avatar}"
            },
            "thumbnail": {
                "url": "{actor_avatar}"
            }
        }
    ],
    "username": "GitHub Action",
    "attachments": []
}
